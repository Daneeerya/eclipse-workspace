package framework_oop_prods;
public abstract class product {
	private int stock;
	private int num_dies_promo;
	private float weight ;
	private String ID_prod ;
	private String fecha_promo;
	private String f_sales;
	private float price_promo;
	private int num_dias_sales;
 	private float price_sales;
 	
 	private String f_ult_unitates;
 	private int	num_dies_ultimes_unitats;
 	private float	price_ult_unitats;
//Constructor
	public product(int stock, float weight, String iD_prod) {
		super();
		this.stock = stock;
		this.weight = weight;
		this.ID_prod = iD_prod;
	}

	public int getStock() {
		return stock;
	}

	public void setStock(int stock) {
		this.stock = stock;
	}

	public float getWeight() {
		return weight;
	}

	public void setWeight(double newWeight) {
		this.weight = (float) newWeight;
	}

	public String getID_prod() {
		return ID_prod;
	}

	public void setID_prod(String iD_prod) {
		this.ID_prod = iD_prod;
	}

	@Override
	public abstract String toString();
	
}
